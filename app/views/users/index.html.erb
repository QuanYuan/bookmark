<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= link_to "New Bookmark", signup_path, class: "btn btn-large btn-primary" %>
<ul class="users">
  <% @users.each do |user| %>
    <li>

      <%= link_to user.name, user %>
      <%= link_to "Edit", edit_user_path(user), class: "btn btn-large btn-primary" %>
      <%= link_to "delete", user, method: :delete,
                  data: { confirm: "You sure?" }, class: "btn btn-large btn-primary" %>
    </li>
  <% end %>
</ul>